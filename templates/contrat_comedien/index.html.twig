{% extends 'base.html.twig' %}

{% block title %}ContratComedien index{% endblock %}
{% block javascripts %}{% endblock %}
{% block body %}
    <h1>Tous vos contrats avec les com√©diens</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id_contrat_comedien</th>
                <th>Creation_contrat</th>
                <th>Date_debut</th>
                <th>Date_fin</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for contrat_comedien in contrat_comediens %}
            <tr>
                <td>{{ contrat_comedien.idContratComedien }}</td>
                <td>{{ contrat_comedien.creationContrat ? contrat_comedien.creationContrat|date('Y-m-d') : '' }}</td>
                <td>{{ contrat_comedien.dateDebut ? contrat_comedien.dateDebut|date('Y-m-d') : '' }}</td>
                <td>{{ contrat_comedien.dateFin ? contrat_comedien.dateFin|date('Y-m-d') : '' }}</td>




                <td>
                    <a href="{{ path('app_contrat_comedien_show', {'id_contrat_comedien': contrat_comedien.idContratComedien}) }}">show</a>
                    <a href="{{ path('app_contrat_comedien_edit', {'id_contrat_comedien': contrat_comedien.idContratComedien}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>


{% endblock %}
